<UserControl x:Class="ARK.View.Administrationssystem.Filters.FormsFilter"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:VMFilter="clr-namespace:ARK.ViewModel.Administrationssystem.Filters"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <UserControl.DataContext>
        <VMFilter:FormsFilterViewModel></VMFilter:FormsFilterViewModel>
    </UserControl.DataContext>
    
    <StackPanel>
        <CheckBox Content="Langtur"></CheckBox>
        <CheckBox Content="Skader"></CheckBox>
        <Separator Height="20"></Separator>
        <CheckBox Content="Afviste"></CheckBox>
        <CheckBox Content="Godkendte"></CheckBox>
    </StackPanel>
</UserControl>

<UserControl x:Class="ARK.View.Administrationssystem.Filters.BoatsFilter"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:VMFilter="clr-namespace:ARK.ViewModel.Administrationssystem.Filters"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <UserControl.DataContext>
        <VMFilter:BoatFilterViewModel></VMFilter:BoatFilterViewModel>
    </UserControl.DataContext>
    <StackPanel>
        <CheckBox Content="Både ude"></CheckBox>
        <CheckBox Content="Både hjemme"></CheckBox>
        <Separator Height="20"></Separator>
        <CheckBox Content="Både under reparation"></CheckBox>
        <CheckBox Content="Beskadige både"></CheckBox>
        <CheckBox Content="Inaktive både"></CheckBox>
        <CheckBox Content="Funktionelle både"></CheckBox>
    </StackPanel>
</UserControl>

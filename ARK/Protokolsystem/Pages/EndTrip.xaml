             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             xmlns:VM="clr-namespace:ARK.ViewModel"
             d:DesignHeight="300" d:DesignWidth="300" Width="600" Height="600">
    <UserControl.DataContext>
        <VM:EndTripViewModel />
    </UserControl.DataContext>
    <Grid Height="600">
        <ListView x:Name="EndTripBoatsOutList" ItemsSource="{Binding }" HorizontalAlignment="Left" Height="600" VerticalAlignment="Top" Width="295" Margin="0,0,0,-3">
            <ListView.ItemTemplate>
                <DataTemplate>
                    <WrapPanel>
                        <!--<TextBlock Text="{Binding Path=}"-->
                    </WrapPanel>
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
        <ListView x:Name="BoatsOutAvailableTrips" HorizontalAlignment="Left" Height="600" VerticalAlignment="Top" Width="295" Margin="305,0,0,0">
			<ListViewItem>
				Jørgen Svendsen
			</ListViewItem>
        </ListView>

    </Grid>
</UserControl>

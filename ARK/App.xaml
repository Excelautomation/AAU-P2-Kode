<Application x:Class="ARK.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Startup="Application_Startup"
             DispatcherUnhandledException="Application_DispatcherUnhandledException">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/View/Resources/Default.xaml"/>
                <ResourceDictionary Source="/View/Resources/Textbox.xaml"/>
                <ResourceDictionary Source="/View/Resources/Buttons.xaml"/>
                <ResourceDictionary Source="/View/Resources/Styles.xaml"/>
                <ResourceDictionary Source="/View/Resources/Search.xaml"/>
                <ResourceDictionary Source="/View/Resources/RadioButton.xaml"/>
                <ResourceDictionary Source="/View/Resources/AdditionalInfo.xaml" />
                <ResourceDictionary Source="/View/Resources/ListView.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
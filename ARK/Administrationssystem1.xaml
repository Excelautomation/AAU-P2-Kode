<Window xmlns:Administrationssystem="clr-namespace:Administrationssystem"  x:Class="ARK.Administrationssystem"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Aalborg Roklubs administrationssystem" Height="768" Width="1024"
        MinWidth="800" MinHeight="600">
    <DockPanel>
        <Administrationssystem:TopMenu DockPanel.Dock="Top" />
        <DockPanel>
            <Administrationssystem:Filter Margin="10,10,10,0" DockPanel.Dock="Left" VerticalAlignment="Top" MinWidth="200" Loaded="Filter_Loaded"/>
            <Administrationssystem:Content Margin="10,10,0,0" />
        </DockPanel>
    </DockPanel>
</Window>
